<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>pl.zarajczyk.my-family</string>
		<key>Program</key>
		<string>$HOME/Library/my-family/my-family.sh</string>
		<key>RunAtLoad</key>
		<true/>
		<key>StandardErrorPath</key>
		<string>$HOME/Library/my-family/stderr.log</string>
		<key>StandardOutPath</key>
		<string>$HOME/Library/my-family/stdout.log</string>
		<key>KeepAlive</key>
		<true/>
	</dict>
</plist>